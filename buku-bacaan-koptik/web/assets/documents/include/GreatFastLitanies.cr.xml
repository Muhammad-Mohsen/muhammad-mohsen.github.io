          </Role>            <Role id="Deacon">         <Text>            <Language id="Coptic">               Twbh e~jen n/etause e~p~semmo.            </Language>            <Language id="English">               Pray for the travelers.            </Language>            <Language id="Arabic">               ØµÙ„ÙˆØ§ Ù…Ù† Ø£Ø¬Ù„ Ø§Ù„Ù…Ø³Ø§Ù?Ø±ÙŠÙ†.            </Language>         </Text>      </Role>            <InsertDocument path="include/LordHaveMercy"/>            <InsertDocument path="include/LetUsBow"/>            <Role id="Priest">         <InsertDocument path="include/SilentPrayerGospel.4"/>      </Role>            <Role id="Deacon">         <Text>            <Language id="Coptic">               Twbh e~jen pia~/r eynanef nem nikarpoc n~te p~kahi.            </Language>            <Language id="English">               Pray for the good air and fruits of the earth.            </Language>            <Language id="Arabic">               ØµÙ„ÙˆØ§ Ù…Ù† Ø£Ø¬Ù„ Ø§Ù„Ù‡ÙˆØ§Ø¡ Ø§Ù„ØµØ§Ù„Ø­ ÙˆØ«Ù…Ø§Ø± Ø§Ù„Ø£Ø±Ø¶.            </Language>         </Text>      </Role>            <InsertDocument path="include/LordHaveMercy"/>            <Role id="Priest">         <InsertDocument path="include/SilentPrayerGospel.5"/>            </Role>            <Role id="Deacon">         <Text>            <Language id="Coptic">               Twbh e~jen p~jinmosi e~p~swi n~te niiarwou m~mwou nem nefjini~ e~p~swi kata nousi.            </Language>            <Language id="English">               Pray for the rising of the waters of the rivers according to their measure.            </Language>            <Language id="Arabic">               ØµÙ„ÙˆØ§ Ù…Ù† Ø£Ø¬Ù„ ØµØ¹ÙˆØ¯ Ù…ÙŠØ§Ù‡ Ø§Ù„Ø£Ù†Ù‡Ø§Ø±ØŒ ÙˆØ§Ø±ØªÙ?Ø§Ø¹Ù‡Ø§ ÙƒÙ…Ù‚Ø¯Ø§Ø±Ù‡Ø§.            </Language>         </Text>      </Role>            <InsertDocument path="include/LordHaveMercy"/>            <Role id="Priest">         <InsertDocument path="include/SilentPrayerGospel.6"/>            </Role>            <Role id="Deacon">         <Text>            <Language id="Coptic">               Twbh e~jen nimounhwou eynanef nem nirwt n~te p~kahi.            </Language>            <Language id="English">               Pray for good rain and the plants of the earth.            </Language>            <Language id="Arabic">               ØµÙ„ÙˆØ§ Ù…Ù† Ø£Ø¬Ù„ Ø§Ù„Ø£Ù…Ø·Ø§Ø± Ø§Ù„ØµØ§Ù„Ø­Ø© ÙˆÙ…Ø²Ø±ÙˆØ¹Ø§Øª Ø§Ù„Ø£Ø±Ø¶.            </Language>         </Text>      </Role>            <InsertDocument path="include/LordHaveMercy"/>            <InsertDocument path="include/LetUsBow"/>            <Role id="Priest">         <InsertDocument path="include/SilentPrayerGospel.7"/>            </Role>            <Role id="Deacon">         <Text>            <Language id="Coptic">               Twbh e~jen v~nohem n~te nirwmi nem nitebnwoui.            </Language>            <Language id="English">               Pray for the salvation of men and animals.            </Language>            <Language id="Arabic">               ØµÙ„ÙˆØ§ Ù…Ù† Ø£Ø¬Ù„ Ù†Ø¬Ø§Ø© Ø§Ù„Ù†Ø§Ø³ ÙˆØ§Ù„Ø¯ÙˆØ§Ø¨.            </Language>         </Text>      </Role>            <InsertDocument path="include/LordHaveMercy"/>            <Role id="Priest">         <InsertDocument path="include/litanies/PlaceShortPriest"/>            </Role>            <Role id="Deacon">         <Text>            <Language id="Coptic">               Twbh e~jen t~cwt/ria m~pikocmoc.            </Language>            <Language id="English">               Pray for the salvation of the world.            </Language>            <Language id="Arabic">               ØµÙ„ÙˆØ§ Ù…Ù† Ø£Ø¬Ù„ Ø®Ù„Ø§Øµ Ø§Ù„Ø¹Ø§Ù„Ù….            </Language>         </Text>      </Role>            <InsertDocument path="include/LordHaveMercy"/>            <Role id="Priest">         <InsertDocument path="include/SilentPrayerGospel.8"/>            </Role>            <Role id="Deacon">         <Text>            <Language id="Coptic">               Twbh e~jen nenourwou (nenar,/) m~mai,~rictoc.            </Language>            <La